{
    "containerDefinitions": [
        {
            "command": [
                "/app/start.sh"
            ],
            "essential": true,
            "image": "<IMAGE1_NAME>",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/fargate-task-definition",
                    "awslogs-region": "ap-southeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "name": "sample-website",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                { "name" : "DJANGO_INTERNAL_PORT_NUM", "value" : "80" },
                { "name" : "DJANGO_SETTINGS_MODULE", "value" : "configuration.api.settings.prod" }
            ]
        }
    ],
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::413514076128:role/ecsTaskExecutionRole",
    "family": "fargate-task-definition",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ]
}